<div class="full dark-theme outer">
        <div class="logo-only inner mt-6">
        <form action="/auth/login" method="post">
            <div>
                <label>Username:</label>
                <input type="text" name="username" class="form-control"/>
            </div>
            <div>
                <label>Password:</label>
                <input type="password" name="password" class="form-control"/>
            </div>
            <div class="mt-3">
                <input type="submit" class="btn btn-outline-secondary btn-block search-btn-color" value="Log In"/>
            </div>
        </form>
        <div class="mt-3">
            <a class="btn btn-outline-secondary btn-block search-btn-color" href="/auth/google"><i class="fa fa-google mr-2" aria-hidden="true"></i> Log In with Google</a>
        </div>
    </div>
</div>
